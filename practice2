with open('/Users/jangjiwan/Downloads/sample.json', 'r') as f:
     info = json.load(f)
with open('/Users/jangjiwan/Downloads/sample.json2.txt', 'w',newline="") as output_file:
     f=csv.writer(output_file)
     outZ5003 = info["outJson"]["outZ5003"]
     for fullData in outZ5003:
         for out in fullData["fullData"]:
             if out["frmlNm"] =="소득공제명세서":
                 for rows in out["out"]["data"]["humnDdcSbjsSpecDVOList"]["rows"]:
                     name= rows["fnm"]
                     SSN= rows["resFrgrNo"]
                     print(name, SSN)
                     f.writerow([name,SSN])
 







